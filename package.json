{
  "name": "phuongbinhtourist",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --host 0.0.0.0 --proxy-config=proxy.conf.json",
    "build": "ng build",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server:prod": "ng run phuongbinhtourist:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --configuration=production",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:browser:serverless": "ng build --prod --base-href /",
    "build:report": "ng build --aot --prod --stats-json && webpack-bundle-analyzer dist/phuongbinhtourist/stats.json",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "cp-cli dist/ functions/dist/ && cd functions && npm install && firebase deploy",
    "build:server:serverless": "ng run phuongbinhtourist:server && webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.7",
    "@angular-devkit/architect": "^0.13.9",
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ng-toolkit/serverless": "1.1.28",
    "@ng-toolkit/universal": "^1.1.51",
    "@nguniversal/common": "~7.0.0",
    "@nguniversal/express-engine": "~7.0.0",
    "@nguniversal/module-map-ngfactory-loader": "~7.0.0",
    "angularfire2": "^5.2.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "es6-crawler-detect": "^3.0.0",
    "firebase": "5.7.0",
    "isbot": "^2.4.0",
    "jquery": "^3.4.1",
    "ng2-search-filter": "^0.4.7",
    "ngx-color-picker": "^7.5.0",
    "ngx-owl-carousel": "^2.0.7",
    "ngx-pagination": "^3.3.1",
    "nodemailer": "^2.7.2",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.5.2",
    "ts-loader": "4.2.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "opencollective": "^1.0.3",
    "protractor": "~5.4.0",
    "ts-loader": "4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.3.6",
    "ws": "^6.2.1",
    "xmlhttprequest": "^1.8.0"
  }
